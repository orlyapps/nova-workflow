{
    "name": "orlyapps/nova-workflow",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0 | ^8.1",
        "illuminate/support": "8.* || 9.* || 10.*",
        "spatie/laravel-ray": "^1.32",
        "symfony/event-dispatcher": "^5.0",
        "symfony/workflow": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Orlyapps\\NovaWorkflow\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orlyapps\\NovaWorkflow\\NovaWorkflowServiceProvider"
            ],
            "aliases": {
                "Workflow": "Orlyapps\\NovaWorkflow\\Facades\\WorkflowFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
